{
  "manifest_version": 2,
  "name": "data-test getter",
  "description": "data-test属性を取得して表示、クリップボードにコピーを行います",
  "version": "1.0.0",
  "browser_action": {
    "default_icon": "icon16.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": []
}